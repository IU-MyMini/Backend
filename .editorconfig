# Xml files
[*.{xml,csproj}]
indent_size = 4

# Allow chaining in Context files
[**/*Context.cs]
resharper_csharp_align_multiline_calls_chain = true
resharper_wrap_chained_method_calls = chop_always

[**/AppDbContext.cs]
resharper_csharp_align_multiline_calls_chain = false
resharper_wrap_chained_method_calls = chop_if_long


# Ignore hard line limit in dtos, responses files
[**/*{Response,Dto}.cs]
resharper_csharp_max_line_length = 1000

# Controllers more readable
[**/*Controller.cs]
resharper_max_formal_parameters_on_line = 1

# Ignore new specifier in Errors.cs files
[**/Errors.cs]
resharper_object_creation_when_type_evident = target_typed
resharper_object_creation_when_type_not_evident = target_typed

# Ignore paths
[{**/Migrations/*,ApiClients/*}]
generated_code = true

# jsonnet files
[*.jsonnet]
indent_size = 2

[**/HandleUpdate.cs]
resharper_csharp_max_line_length = 130